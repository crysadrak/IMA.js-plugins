    "directory": "packages/cli-plugin-less-constants"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/seznam/IMA.js-plugins/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "npm": ">=7"
  },
