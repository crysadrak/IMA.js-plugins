{
  "name": "@ima/cli-plugin-less-constants",
  "version": "1.0.0-rc.4",
  "description": "Plugin for @ima/cli to enable sharing constants between LESS and JS source files.",
  "main": "./dist/cjs/main.js",
  "module": "./dist/esm/main.js",
  "types": "./dist/esm/main.d.ts",
  "exports": {
    ".": {
      "main": "./dist/cjs/main.js",
      "module": "./dist/esm/main.js",
