{
  "name": "@ima/plugin-testing-integration",
  "version": "2.1.0",
  "description": "IMA.js plugin for integration testing",
  "main": "dist/main",
  "module": "dist/main",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs"
    },
    "./EnzymePageRenderer": "./EnzymePageRenderer.js",
    "./EnzymePageRenderer.js": "./EnzymePageRenderer.js"
  },
  "browser": {
    "./dist/main.js": "./dist/main.es5.js",
    "./dist/main.cjs": "./dist/main.es5.js",
    "./dist/main.mjs": "./dist/main.mjs"
  },
  "scripts": {
    "build": "echo \"Release process is supported only via lerna command. See README.md for more info.\"; exit 1",
    "test": "../../node_modules/.bin/jest --coverage --no-watchman --config=jest.config.js",
    "test:es:version": "../../node_modules/.bin/es-check es5 ./dist/main.es5.js && ../../node_modules/.bin/es-check --module es9 ./dist/main.mjs",
    "doc": "../../node_modules/.bin/gulp doc",
    "prepare": "../../node_modules/.bin/rollup -c"
  },
  "keywords": [
    "IMA.js",
    "Testing",
    "Integration",
    "Module",
    "Javascript"
  ],
  "author": "Filip Satek <filip.satek@firma.seznam.cz>",
  "contributors": [
    {
      "name": "Miroslav Jancarik",
      "email": "miroslav.jancarik@firma.seznam.cz"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/seznam/IMA.js-plugins.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/seznam/IMA.js-plugins/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">=4 <8"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "@ima/helpers": "^17.4.0",
    "globby": "^11.0.0",
    "jsdom": "^16.2.1",
    "messageformat": "^2.3.0",
    "to-aop": "^0.5.3"
  },
  "peerDependencies": {
    "@ima/core": ">= 17.0.0"
  }
}
